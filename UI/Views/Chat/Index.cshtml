@using Humanizer;
@model Sheet

<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/magnific-popup.js/1.1.0/magnific-popup.css" />

@{
    ViewData["Title"] = "Chats";
    var stickers = (List<StickerGroup>)ViewData["stickers"] ?? new List<StickerGroup>();
}

@if (Model == null)
{
    <div class="alert alert-danger text-center">No chats</div>
}
else
{
    <input type="hidden" id="sheetId" value="@Model.Id" />
    <input type="hidden" id="ownerId" value="@Model.Identity" />

    <partial name="_AlertsPartial" />

    <div class="container pb-3">

        <div class="row">

            <div class="col-md-12">

                <div class="card" id="chat" style="border-radius: 15px;">
                    <div class="card-body">
                        <div class="row">

                            <div class="col-md-6 col-lg-5 col-xl-4 mb-4 mb-md-0">
                                <div class="p-3">

                                    <div id="divSearch" class="input-group mb-3">
                                        <span class="input-group-text border-0">
                                            <i class="fa-solid fa-search"></i>
                                        </span>
                                        <input type="search" id="search" class="form-control rounded" oninput="searchDialogue(this)" placeholder="Search" aria-label="search" aria-describedby="search" autocomplete="off" />
                                    </div>

                                    <div>
                                        <!-- Tabs navs -->
                                        <ul class="nav nav-tabs nav-justified mb-1" id="tabsNavs" role="tablist">
                                            <li class="nav-item" role="presentation">
                                                <a class="nav-link active" id="tab-actibe" data-mdb-toggle="tab" role="tab" aria-selected="true" data-type="active" onclick="getDialogues(true)">
                                                    Active
                                                </a>
                                            </li>
                                            <li class="nav-item" role="presentation">
                                                <a class="nav-link" id="tab-all" data-mdb-toggle="tab" role="tab" aria-selected="false" data-type="bookmarked" onclick="getDialogues(true)">
                                                    Saved
                                                </a>
                                            </li>
                                            <li class="nav-item" role="presentation">
                                                <a class="nav-link" id="tab-all" data-mdb-toggle="tab" role="tab" aria-selected="false" data-type="" onclick="getDialogues(true)">
                                                    All
                                                </a>
                                            </li>
                                        </ul>
                                        <div id="onlineOnly" class="row mb-2">
                                            <div class="col">
                                                <label class="form-check-label" for="flexSwitchCheckDefault">Online only</label>
                                            </div>
                                            <div class="form-check form-switch col-auto">
                                                <input class="form-check-input" type="checkbox" role="switch" id="isOnlineOnly" onchange="getDialogues(true)" />
                                            </div>
                                        </div>
                                        <!-- Tabs navs -->
                                        <!-- Tabs content -->
                                        <div class="tab-content overflow-auto" id="usersArea" style="position: relative;" onscroll="additionalLoadDialogues(this)">
                                            <ul class="list-unstyled list-group mb-0" id="listUsers" data-cursor="" data-all-load="false">
                                                @await Component.InvokeAsync("Dialogues", new {sheet = Model, criteria = "active"})
                                            </ul>
                                        </div>
                                        <!-- Tabs content -->
                                    </div>

                                </div>

                            </div>

                            <div id="messagesArea" class="col-md-6 col-lg-7 col-xl-8 py-3">
                                <div id="plugChatMenu" class="mb-1"></div>
                                <div id="plugMessages" class="p-3"></div>

                                <!-- Message Editor -->
                                <div id="messageCreationArea" class="position-relative">

                                    <!-- Emojis -->
                                    <div class="position-relative">
                                        <div id="emojisDiv" class="position-absolute bottom-100 d-none">
                                            <div id="emojis">
                                            </div>
                                        </div>
                                    </div>

                                    <!-- Stickers -->
                                    <div class="position-relative">
                                        <div id="stickers" class="position-absolute bg-white bottom-100 rounded-top d-none">
                                            <div class="modal-header sticky-top bg-primary px-2 rounded-top">
                                                <h5 class="modal-title text-white">Stickers</h5>
                                                <button type="button" class="btn-close" data-mdb-dismiss="modal" aria-label="Close" onclick="hidenStickers()"></button>
                                            </div>
                                            <div class="row mx-0 bg-light" style="height: 33vh; overflow-y: auto; overflow-x: hidden;">
                                                @foreach (var stickerGroup in stickers)
                                                {
                                                    <div class="col-12">
                                                        @stickerGroup.Name
                                                    </div>
                                                    @foreach (var sticker in stickerGroup.Stickers)
                                                    {
                                                        <div class="bg-image hover-overlay ripple col-2">
                                                            <img src="@sticker.Url" class="img-fluid" />
                                                            <a href="#!" onclick="sendSticker(this, @sticker.Id)">
                                                                <div class="mask" style="background-color: rgba(57, 192, 237, 0.2);"></div>
                                                            </a>
                                                        </div>
                                                    }
                                                }
                                            </div>
                                        </div>
                                    </div>

                                    <!-- Text Message -->
                                    <div id="messageSend" class="text-muted d-flex justify-content-start align-items-center pt-3">
                                        <img id="ownerAvatar" src="@ViewData["avatar"]" alt="owner" class="rounded-circle d-flex" style="width: 40px; height: 100%" />
                                        <div class="form-outline ms-3">
                                            <textarea type="text" rows="2" class="form-control form-control-lg" id="newMessage" placeholder="Message" data-mdb-showcounter="true" maxlength="300" onkeydown="pressEnter(event, @Model.Identity)" style="resize: none;"></textarea>
                                            <div class="form-helper"></div>
                                        </div>
                                        <a class="ms-3 btn btn-primary" role="button" href="#!" onclick="sendMessage()" title="Send"><i class="fa-solid fa-paper-plane fa-xl"></i></a>
                                    </div>

                                    <!-- Nav Content Message -->
                                    <div id="contentSend" class="d-flex mt-4">
                                        <div id="btnEmojis" class="d-grid col-4 ps-0">
                                            <a class="btn btn-warning" role="button" href="#!" onclick="showHidenEmojis()" title="Emojis">Emojis<i class="fa-solid fa-smile fa-2xl ms-1"></i></a>
                                        </div>
                                        <div id="btnStickers" class="d-grid col-4">
                                            <a class="ms-3 btn text-white" href="#!" onclick="showHidenStickers()" title="Stickers" style="background-color: hotpink;">Stickers<i class="fa-solid fa-face-kiss-wink-heart fa-2xl ms-1"></i></a>
                                        </div>
                                        <div class="d-grid col-4">
                                            <button type="button" class="ms-3 btn btn-danger" href="#!" title="Media" data-mdb-toggle="modal" data-mdb-target="#mediaModal">
                                                Media
                                                <i class="fa-solid fa-camera-retro fa-2xl ms-1"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>

                            </div>

                        </div>
                    </div>
                </div>

            </div>

        </div>

    </div>

    <div class="modal fade" id="modalComments" tabindex="-1" role="dialog" aria-hidden="true">
        <div class="modal-dialog modal-dialog-scrollable">
            <div class="modal-content gradient-custom">
            </div>
        </div>
    </div>

    <!-- Modal Media -->
    <div class="modal fade" id="mediaModal" tabindex="-1" aria-labelledby="mediaModalLabel" aria-hidden="true">
