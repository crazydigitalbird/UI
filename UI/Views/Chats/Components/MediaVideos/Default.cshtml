@model Media

<input type="hidden" id="new-cursor-videos" value="@Model?.Cursor" />
@if (Model != null && Model.Videos?.Count > 0)
{
    @foreach (var video in Model.Videos)
    {
        <div class="gallery-card video" data-id="@video.IdVideo" data-url-original="@video.Urls.UrlMp4Hd">
            <img src="@video.Urls.UrlThumbnail" alt="">
            <div>
                <svg name="play-svg" class="" width="35" height="35" viewBox="0 0 35 35" fill="none" xmlns="http://www.w3.org/2000/svg">
                    <path d="M17.5 35C7.83243 35 0 27.1676 0 17.5C0 7.83243 7.83243 0 17.5 0C27.1676 0 35 7.83243 35 17.5C35 27.1676 27.1676 35 17.5 35Z" fill="#7D6AF0"></path>
                    <path d="M24.5819 14.5531L16.1951 10.3141C14.0007 9.21723 11.3855 10.7884 11.3855 13.2192V21.6973C11.3855 24.1281 14.0007 25.6993 16.1951 24.6024L24.5819 20.3634C26.9867 19.148 26.9867 15.7685 24.5819 14.5531Z" fill="white"></path>
                </svg>
            </div>
            <svg width="20" height="20" viewBox="0 0 20 20" fill="none" xmlns="http://www.w3.org/2000/svg">
                <path d="M10 19.25C4.89 19.25 0.75 15.11 0.75 10C0.75 4.89 4.89 0.75 10 0.75C15.11 0.75 19.25 4.89 19.25 10C19.25 15.11 15.11 19.25 10 19.25Z" fill="#7D6AF0"></path>
                <path d="M9.54027 11.5803L7.78027 9.82031" stroke="white" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"></path>
                <path d="M13.06 8.06006L9.54004 11.5801" stroke="white" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"></path>
            </svg>
        </div>
    }
}
else
{
    @*<div class="text-center">No videos</div>*@
}